eact'\r\nimport ItemCard from '../Components/ItemCard'\r\nimport Loader from '../Components/Loader'\r\nimport { useAxiosGet } from '../Hooks/HttpRequest'\r\n\r\nfunction Home() {\r\n    const url = `https://5e9623dc5b19f10016b5e31f.mockapi.io/api/v1/products?page=1&limit=5`\r\n    //calling wateva is returned from the custom HOOK request\r\n    //with an argument of the variable used for the request (i.e URL)\r\n    const [searchTerm, setSearchTerm] = useState('')\r\n    let Products = useAxiosGet(url)\r\n\r\n    let content = null\r\n\r\n    if (Products.error) {\r\n        content = <p>There was an ERROR. Try again</p>\r\n    }\r\n\r\n    if (Products.loading) {\r\n        content = <Loader />\r\n    }\r\n\r\n    if (Products.data) {\r\n        //To loop through an ARRAY, we use variable.data.map\r\n        /*This (.map) accepts 2 arguments: \r\nthe singular name for the Data being looped through and Key(Index of the item)*/\r\n        content =\r\n            Products.data.filter((item) => {\r\n                if (searchTerm === \"\") {\r\n                    return item\r\n                } else if (item.name.toLowerCase().includes(searchTerm.toLowerCase())) {\r\n                    return item\r\n                }\r\n            }).map((item) =>\r\n                <div key={item.id}>\r\n                    <ItemCard product={item} />\r\n                </div>\r\n            )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <header className=\"flex justify-between items-center mb-3\">\r\n                <h1 className=\"font-bold text-2xl\">\r\n                    weGet\r\n                </h1>\r\n                <input\r\n                    className=\"pl-2 border rounded overflow-hidden\"\r\n                    type=\"text\"\r\n                    placeholder=\"Search...\"\r\n                    onChange={(event) => {\r\n                        setSearchTerm(event.target.value)\r\n                    }} />\r\n            </header>\r\n\r\n            {content}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home\r\n","C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\Items.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Components\\Loader.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Components\\ItemCard.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Hooks\\HttpRequest.js",["84"],"import axios from 'axios'\r\nimport React, { useEffect, useState } from 'react'\r\n\r\nexport function useAxiosGet(url) {\r\n    const [request, setRequest] = useState({\r\n        loading: false,\r\n        data: null,\r\n        error: false\r\n    })\r\n//The hook checks the request and returns RESPONSE received from the API.\r\n//Second, a variiable to monitor for any change to rerun the function code\r\n    useEffect(() => {\r\n//Initial request sent\r\n        setRequest({loading: true, data: null, error: false})\r\n        axios.get(url)\r\n//when response is successfully fetched\r\n            .then(Response => {\r\n                setRequest({\r\n                    loading: false, \r\n                    data: Response.data, \r\n                    error: false})\r\n            })\r\n//if error is encountered from the API\r\n//report the Error response from API as Error.data\r\n            .catch(Error => {\r\n                setRequest({\r\n                    loading: false, \r\n                    data: null, \r\n                    error: true})\r\n            })\r\n    }, [url])\r\n\r\n    return request\r\n}","C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\ReqPage.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\Extra\\Forms.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\Login.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\Request.js",[],"C:\\Users\\PIERO\\Documents\\project-x\\src\\Views\\About.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":28,"column":41,"nodeType":"91","